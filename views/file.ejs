<form action="/file/upload" enctype="multipart/form-data" method="POST">
    <input type="file" name="filename" placeholder="filename">
    <input type="submit"  value="Upload">
</form>
<% for(f of files){%>
    <p>
        <a href="<%=f.filename%>"><%= f.filename %></a>
    </p>
<% } %>
